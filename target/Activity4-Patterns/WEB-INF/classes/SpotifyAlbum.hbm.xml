<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nilfactor.activity4.model.SpotifyAlbum" table="spotify_album" schema="public">
       	<id name="albumId" type="string" column="album_id"/>
       	<property name="imageLink" column="image_link"/>
	   	<property name="imageWidth" column="image_width"/>
 	   	<property name="imageHeight" column="image_height"/>
       	<property name="totalTracks" column="total_tracks"/>
       	<property name="artists" column="artists" type="string"/>
       	<property name="releaseDate" column="release_date"/>
       	<property name="name" column="name"/>
       	<property name="link" column="link"/>
    </class>
</hibernate-mapping>